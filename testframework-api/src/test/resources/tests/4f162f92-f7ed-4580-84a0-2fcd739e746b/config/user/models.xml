<?xml version="1.0" encoding="UTF-8"?>

<models>

    <model>
        <simulatorID>Simulator1</simulatorID> <!-- name as specified in simulators.xml/simulators/simulator/id -->
        <modelID>Model1</modelID>
        <modelIndex>0</modelIndex>
        <additionalModelParams>
            <param>
                <key>init_val</key>
                <value>10</value>
            </param>
        </additionalModelParams>
    </model>

    <model>
        <simulatorID>Simulator1</simulatorID> <!-- name as specified in simulators.xml/simulators/simulator/id -->
        <modelID>Model1</modelID>
        <modelIndex>1</modelIndex>
        <additionalModelParams>
            <param>
                <key>init_val</key>
                <value>-8</value>
            </param>
        </additionalModelParams>
    </model>

    <model>
        <simulatorID>Simulator2</simulatorID> <!-- name as specified in simulators.xml/simulators/simulator/id -->
        <modelID>Model2</modelID>
        <modelIndex>0</modelIndex>
        <additionalModelParams>
            <param>
                <key>init_val</key>
                <value>-10</value>
            </param>
        </additionalModelParams>
    </model>

    <model>
        <simulatorID>Simulator2</simulatorID> <!-- name as specified in simulators.xml/simulators/simulator/id -->
        <modelID>Model2</modelID>
        <modelIndex>1</modelIndex>
        <additionalModelParams>
            <param>
                <key>init_val</key>
                <value>-500</value>
            </param>
        </additionalModelParams>
    </model>

    <!-- global test evaluator generated by framework -->

    <model>
        <simulatorID>GlobalTestEvaluatorSimulator</simulatorID> <!-- name as specified in simulators.xml/simulators/simulator/id -->
        <modelID>ModelGlobalTestEvaluator</modelID>
        <modelIndex>0</modelIndex>
        <additionalModelParams>
            <param>
                <key>__StandaloneTestEvaluator__</key>
                <value>true</value>
            </param>
            <param>
                <key>__PermanentFailureEntry__</key>
                <value>${globalTestEvalPermanentFailureEntry}</value>
            </param>
            <param>
                <key>__TestResultName__</key>
                <value>${testResultName}</value>
            </param>
            <param>
                <key>__TestResultPath__</key>
                <value>${testEvalInjectedPath}</value>
            </param>
        </additionalModelParams>
    </model>

</models>